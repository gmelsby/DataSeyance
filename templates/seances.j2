<div id="insert" style="display: block">
  <form id="InsertSeance">
      <legend><strong>Insert Seance</strong></legend>
     <fieldset class="fields">
            <label> Date: </label> <input type="date"  name="date">
       <label for="newloc">Location: </label>
      <select name="newloc" id="newloc">
          <option value="None"></option>
        <option value="Barnum"> Barnum's Hotel | id: 1</option>
        <option value="Beach"> Beachland Ballroom & Tavern | id: 2</option>
      </select>
    </fieldset>
    <input class="btn" type="submit" id="InsertSeance" value="Insert Seance">
  </form>
</div>
